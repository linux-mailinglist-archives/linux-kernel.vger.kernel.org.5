>>  .../marvell/octeontx2/af/rvu_npc_fs.c         |  13 +++
>>  .../ethernet/marvell/octeontx2/nic/otx2_tc.c  | 106
>> +++++++++++-------
>>  5 files changed, 90 insertions(+), 38 deletions(-)
>
><...>
>
>> +	if (!is_inner)
>> +		flow_rule_match_vlan(rule, &match);
>> +	else
>> +		flow_rule_match_cvlan(rule, &match);
